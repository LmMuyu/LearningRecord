
.imageUploadContainer {
  padding: 10rpx 5rpx;
  margin: 10rpx 5rpx;
}
.dragging {
  -webkit-transform: scale(1.2);
          transform: scale(1.2);
}
.deleteTu {
  position: absolute;
  top:-9rpx;
  right: -6rpx;
  font-size: 40rpx;
}
.imageUploadList {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.imageItem,
.imageUpload {
  width: 160rpx;
  height: 160rpx;
  margin: 10rpx;
  position: relative;
}
.imageItem image,
.moveImage {
  width: 160rpx;
  height: 160rpx;
  border-radius: 8rpx;
}
.imageUpload {
  line-height: 130rpx;
  text-align: center;
  font-size: 150rpx;
  color: #d9d9d9;
  border: 1px solid #d9d9d9;
  border-radius: 8rpx;
}
.moveImage {
  position: absolute;
}

